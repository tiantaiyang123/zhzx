<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhzx.server.repository.base.ExamPrintLogBaseMapper">
    <resultMap id="BaseResultMap" type="com.zhzx.server.domain.ExamPrintLog">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="exam_publish_id" jdbcType="BIGINT" property="examPublishId" />
        <result column="student_id" jdbcType="INTEGER" property="studentId" />
        <result column="num" jdbcType="INTEGER" property="num" />
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    </resultMap>
    <resultMap id="ResultMap" type="com.zhzx.server.domain.ExamPrintLog" extends="BaseResultMap">
    </resultMap>
    <sql id="Base_Column_List">
        id, exam_publish_id, student_id, num, create_time, update_time
    </sql>
    <sql id="Base_Column_List_With_Prefix">
        t.id, t.exam_publish_id, t.student_id, t.num, t.create_time, t.update_time
    </sql>
    <insert id="batchInsert">
        insert into dat_exam_print_log (
            exam_publish_id, student_id, num
        )
        values
        <foreach collection="records" item="record" separator=",">
        (
            #{record.examPublishId, jdbcType=BIGINT}, #{record.studentId, jdbcType=INTEGER}, #{record.num, jdbcType=INTEGER}
        )
        </foreach>
    </insert>
    <update id="updateAllFieldsById" parameterType="com.zhzx.server.domain.ExamPrintLog">
        update dat_exam_print_log set
        <trim suffixOverrides=",">
            exam_publish_id = #{et.examPublishId, jdbcType=BIGINT},
            student_id = #{et.studentId, jdbcType=INTEGER},
            num = #{et.num, jdbcType=INTEGER},
        </trim>
        where id = #{et.id}
    </update>
</mapper>
