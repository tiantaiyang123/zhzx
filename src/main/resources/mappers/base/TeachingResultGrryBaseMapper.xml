<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhzx.server.repository.base.TeachingResultGrryBaseMapper">
    <resultMap id="BaseResultMap" type="com.zhzx.server.domain.TeachingResultGrry">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="teaching_result_id" jdbcType="BIGINT" property="teachingResultId" />
        <result column="rych" jdbcType="VARCHAR" property="rych" />
        <result column="fzdw" jdbcType="VARCHAR" property="fzdw" />
    </resultMap>
    <resultMap id="ResultMap" type="com.zhzx.server.domain.TeachingResultGrry" extends="BaseResultMap">
        <association column="teaching_result_id" property="teachingResult" javaType="com.zhzx.server.domain.TeachingResult"
                     select="com.zhzx.server.repository.TeachingResultMapper.selectById" />
    </resultMap>
    <sql id="Base_Column_List">
        id, teaching_result_id, rych, fzdw
    </sql>
    <sql id="Base_Column_List_With_Prefix">
        t.id, t.teaching_result_id, t.rych, t.fzdw
    </sql>
    <insert id="batchInsert">
        insert into tmg_teaching_result_grry (
            teaching_result_id, rych, fzdw
        )
        values
        <foreach collection="records" item="record" separator=",">
        (
            #{record.teachingResultId, jdbcType=BIGINT}, #{record.rych, jdbcType=VARCHAR}, #{record.fzdw, jdbcType=VARCHAR}
        )
        </foreach>
    </insert>
    <update id="updateAllFieldsById" parameterType="com.zhzx.server.domain.TeachingResultGrry">
        update tmg_teaching_result_grry set
        <trim suffixOverrides=",">
            teaching_result_id = #{et.teachingResultId, jdbcType=BIGINT},
            rych = #{et.rych, jdbcType=VARCHAR},
            fzdw = #{et.fzdw, jdbcType=VARCHAR},
        </trim>
        where id = #{et.id}
    </update>
</mapper>
