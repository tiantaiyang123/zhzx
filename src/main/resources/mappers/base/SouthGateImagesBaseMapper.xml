<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhzx.server.repository.base.SouthGateImagesBaseMapper">
    <resultMap id="BaseResultMap" type="com.zhzx.server.domain.SouthGateImages">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="south_gate_id" jdbcType="BIGINT" property="southGateId" />
        <result column="image_classify" jdbcType="VARCHAR" property="imageClassify" />
    </resultMap>
    <resultMap id="ResultMap" type="com.zhzx.server.domain.SouthGateImages" extends="BaseResultMap">
        <result column="url" jdbcType="VARCHAR" property="url" />
    </resultMap>
    <sql id="Base_Column_List">
        id, south_gate_id, image_classify, url
    </sql>
    <sql id="Base_Column_List_With_Prefix">
        t.id, t.south_gate_id, t.image_classify, t.url
    </sql>
    <sql id="Base_Column_List_Full_With_Prefix">
        t.id, t.south_gate_id, t.image_classify, t.url
    </sql>
    <insert id="batchInsert">
        insert into day_south_gate_images (
            south_gate_id, image_classify, url
        )
        values
        <foreach collection="records" item="record" separator=",">
        (
            #{record.southGateId, jdbcType=BIGINT}, #{record.imageClassify, jdbcType=VARCHAR}, #{record.url, jdbcType=VARCHAR}
        )
        </foreach>
    </insert>
    <update id="updateAllFieldsById" parameterType="com.zhzx.server.domain.SouthGateImages">
        update day_south_gate_images set
        <trim suffixOverrides=",">
            south_gate_id = #{et.southGateId, jdbcType=BIGINT},
            image_classify = #{et.imageClassify, jdbcType=VARCHAR},
            url = #{et.url, jdbcType=VARCHAR},
        </trim>
        where id = #{et.id}
    </update>
</mapper>
