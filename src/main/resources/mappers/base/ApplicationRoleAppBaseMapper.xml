        <result column="application_app_id" jdbcType="BIGINT" property="applicationAppId" />
    </resultMap>
    <resultMap id="ResultMap" type="com.zhzx.server.domain.ApplicationRoleApp" extends="BaseResultMap">
    </resultMap>
