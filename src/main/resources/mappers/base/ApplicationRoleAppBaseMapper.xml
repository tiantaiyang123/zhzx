        insert into sys_application_role_app (
            role_id, application_app_id
        )
        values
        <foreach collection="records" item="record" separator=",">
        (
            #{record.roleId, jdbcType=BIGINT}, #{record.applicationAppId, jdbcType=BIGINT}
        )
        </foreach>
    </insert>
    <update id="updateAllFieldsById" parameterType="com.zhzx.server.domain.ApplicationRoleApp">
