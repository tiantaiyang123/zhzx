        <trim suffixOverrides=",">
            role_id = #{et.roleId, jdbcType=BIGINT},
            application_app_id = #{et.applicationAppId, jdbcType=BIGINT},
        </trim>
        where id = #{et.id}
    </update>
</mapper>
