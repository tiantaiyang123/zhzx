<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhzx.server.repository.base.DormitoryBaseMapper">
    <resultMap id="BaseResultMap" type="com.zhzx.server.domain.Dormitory">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="name" jdbcType="VARCHAR" property="name" />
        <result column="building" jdbcType="VARCHAR" property="building" />
        <result column="floor" jdbcType="VARCHAR" property="floor" />
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    </resultMap>
    <resultMap id="ResultMap" type="com.zhzx.server.domain.Dormitory" extends="BaseResultMap">
    </resultMap>
    <sql id="Base_Column_List">
        id, name, building, floor, create_time, update_time
    </sql>
    <sql id="Base_Column_List_With_Prefix">
        t.id, t.name, t.building, t.floor, t.create_time, t.update_time
    </sql>
    <update id="updateAllFieldsById" parameterType="com.zhzx.server.domain.Dormitory">
        update sys_dormitory set
        <trim suffixOverrides=",">
            name = #{et.name, jdbcType=VARCHAR},
            building = #{et.building, jdbcType=VARCHAR},
            floor = #{et.floor, jdbcType=VARCHAR},
        </trim>
        where id = #{et.id}
    </update>
</mapper>
